---
import type { InferGetStaticPropsType } from 'astro'

import Page from './components/overrides/Page.astro'
import { getSchemasStaticPaths } from './libs/schema'
import { getPageProps } from './libs/starlight'

export function getStaticPaths() {
  return getSchemasStaticPaths()
}

type Props = InferGetStaticPropsType<typeof getStaticPaths>

const { schema } = Astro.props
---

<Page {...getPageProps('// TODO(HiDeoo)')}>
  <div>{schema.info.version}</div>
</Page>
