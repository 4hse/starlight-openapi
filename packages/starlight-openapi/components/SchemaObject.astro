---
import { getNullable, isObjectSchemaObject, type SchemaObject } from '../libs/schemaObject'
import Items from './Items.astro'
import ObjectSchemaObject from './ObjectSchemaObject.astro'

interface Props {
  schemaObject: SchemaObject
}

const { schemaObject } = Astro.props
---

<Items items={schemaObject} />
{isObjectSchemaObject(schemaObject) && <ObjectSchemaObject {schemaObject} />}
{getNullable(schemaObject) && <div>NULLABLE</div>}
