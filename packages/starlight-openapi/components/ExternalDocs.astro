---
import type { OpenAPIV2, OpenAPIV3, OpenAPIV3_1 } from 'openapi-types'

interface Props {
  docs:
    | OpenAPIV2.ExternalDocumentationObject
    | OpenAPIV3.ExternalDocumentationObject
    | OpenAPIV3_1.ExternalDocumentationObject
    | undefined
}

const { docs } = Astro.props
---

{
  docs && (
    <div>
      <div>External Docs URL: {docs.url}</div>
      {docs.description && <div>External Docs Description: {docs.description}</div>}
    </div>
  )
}
